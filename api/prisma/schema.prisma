generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id String @id @default(uuid())
  origin String
  destination String
  depart_date DateTime
  return_date DateTime
  people Int
  transportation String
  name String
  price Decimal

  @@map("quotes")
}
